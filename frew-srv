fd.pipe	offlineimap-log.r	offlineimap-log.w
service.args	offlineimap-log	tinylog	-t	-k	1	/home/frew/log/offlineimap
service.fds	offlineimap-log	offlineimap-log.r	stderr	stderr
service.auto_up	offlineimap-log	2	always

service.args	offlineimap	offlineimap
service.fds	offlineimap	null	offlineimap-log.w	offlineimap-log.w
service.auto_up	offlineimap	1	always

fd.pipe	syncthing-log.r	syncthing-log.w
service.args	syncthing-log	tinylog	-t	-k	1	/home/frew/log/syncthing
service.fds	syncthing-log	syncthing-log.r	stderr	stderr
service.auto_up	syncthing-log	2	always

service.args	syncthing	/home/frew/Sync/bin/syncthing
service.fds	syncthing	null	syncthing-log.w	syncthing-log.w
service.auto_up	syncthing	1	always

fd.pipe	weather-log.r	weather-log.w
service.args	weather-log	tinylog	-t	-k	1	/home/frew/log/weather
service.fds	weather-log	weather-log.r	stderr	stderr
service.auto_up	weather-log	2	always

service.args	weather	/home/frew/code/dotfiles/bin/w.pl
service.fds	weather	null	weather-log.w	weather-log.w
service.auto_up	weather	1	always

