 autostash lynxapp lynxservice
 
 function lynxapp() {
    plackup -I/home/frew/code/Plack-App-CaseInsensitiveFile/lib -R App/lib,cgi/My,/home/frew/code/plugins/CCure/lib,/home/frew/code/plugins/Foo/lib
 }
 
 function lynxservice() {
    perl Lynx_service.plx
 }
