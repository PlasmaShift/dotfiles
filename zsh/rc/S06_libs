   smartcd setup cd
   smartcd setup pushd
   smartcd setup popd
   smartcd setup prompt-hook
   smartcd setup exit-hook
   smartcd setup completion
   VARSTASH_AUTOCONFIGURE=1
   VARSTASH_AUTOEDIT=1
fi

